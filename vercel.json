{
  "functions": {
    "app/api/scan/route.ts": {
      "maxDuration": 60,
      "memory": 2048
    }
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/cron/end-trials",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/trial-reminders",
      "schedule": "0 9 * * *"
    }
  ]
}
